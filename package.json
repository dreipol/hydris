{
  "name": "hydris",
  "version": "0.0.0",
  "description": "Multithreaded proxy server to handle SSR for SPA",
  "bin": "hydris.js",
  "main": "hydris.js",
  "jsnext:main": "src/index.js",
  "module": "src/index.js",
  "scripts": {
    "prepare": "npm run build && npm test",
    "ci-test": "eslint -c .eslintrc --no-eslintrc -f junit ./ > reports/eslint.xml",
    "lint": "eslint ./",
    "build": "rollup -c",
    "doc": "documentation readme src/index.js -s API",
    "test": "npm run lint && mocha test.js"
  },
  "files": [
    "src/index.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dreipol/hydris.git"
  },
  "keywords": [
    "es6",
    "es2015"
  ],
  "author": "Gianluca Guarini <gianluca.guarini@gmail.com> (http://gianlucaguarini.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dreipol/hydris/issues"
  },
  "homepage": "https://github.com/dreipol/hydris#readme",
  "devDependencies": {
    "@dreipol/eslint-config": "^4.0.0",
    "eslint": "^4.18.2",
    "mocha": "^5.0.1",
    "rollup": "^0.57.1"
  },
  "dependencies": {
    "puppeteer": "^1.2.0"
  }
}
